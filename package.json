{
  "name": "tiktok-signing-paas",
  "version": "1.0.0",
  "description": "TikTok signing Platform-as-a-Service with Next.js frontend and Supabase backend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest --run tests/integration/**/*.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "vitest --run tests/performance/**/*.test.{ts,tsx}",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "setup:dev": "./scripts/setup-environment.sh development",
    "setup:staging": "./scripts/setup-environment.sh staging",
    "setup:prod": "./scripts/setup-environment.sh production",
    "deploy:dev": "./scripts/deploy-development.sh",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:prod": "./scripts/deploy-production.sh",
    "migrate:dev": "./scripts/setup-environment.sh migrate development",
    "migrate:staging": "./scripts/setup-environment.sh migrate staging",
    "migrate:prod": "./scripts/setup-environment.sh migrate production",
    "validate:env": "./scripts/setup-environment.sh validate",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db seed",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.38.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "express": "^5.1.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tiktok-live-connector": "^2.0.7-beta1",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.0",
    "msw": "^2.0.0",
    "postcss": "^8.5.6",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "tiktok",
    "signing",
    "paas",
    "nextjs",
    "supabase",
    "vercel"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
